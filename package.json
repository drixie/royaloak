{
  "name": "vite-reactts-electron-starter",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "Maximilian StÃ¼mpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "Guerilla Trading Platform",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "tsc": "./node_modules/.bin/tsc",
    "postinstall": "electron-rebuild"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^2.14.0",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@master-chief/alpaca": "^6.3.17",
    "@nenjotsu/technicalindicators": "^3.1.3",
    "@radix-ui/react-context-menu": "^0.1.6",
    "@stoqey/ib": "^1.2.30",
    "@tailwindcss/forms": "^0.5.0",
    "@vechaiui/core": "^0.6.0",
    "@vechaiui/react": "^0.2.2",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^0.27.2",
    "better-sqlite3": "^7.6.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "framer-motion": "^6.3.2",
    "lodash.groupby": "^4.6.0",
    "node-schedule": "^2.1.0",
    "numeral": "^2.0.6",
    "quick.db": "^7.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.4",
    "react-hotkeys-hook": "^3.4.4",
    "react-resizable": "^3.0.4",
    "reactn": "^2.2.7",
    "rxjs": "^7.1.0",
    "socket.io-client": "^4.5.0",
    "tailwind-scrollbar": "^1.3.1",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tdsequential": "^1.0.15",
    "uuid": "^8.3.2",
    "vite": "^2.8.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "electron": "^17.1.2",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-rxjs": "^5.0.2",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "rxjs": "^7.1.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
